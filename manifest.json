{
  "name": "TabTags",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Set tags from 1-9 to tabs",
  "homepage_url": "http://www.github.com/hildensia",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "commands": {
    "save-tabtag-1": {
      "suggested_key": {
        "default": "Shift+Ctrl+1"
      },
      "description": "Save current tag to tabtag 1"
    },
    "open-tabtag-1": {
      "suggested_key": {
        "default": "Ctrl+1"
      },
      "description": "Jump to tabtag 1"
    },
    "save-tabtag-2": {
      "suggested_key": {
        "default": "Shift+Ctrl+2"
      },
      "description": "Save current tag to tabtag 2"
    },
    "open-tabtag-2": {
      "suggested_key": {
        "default": "Ctrl+2"
      },
      "description": "Jump to tabtag 2"
    },
    "save-tabtag-3": {
      "description": "Save current tag to tabtag 3"
    },
    "open-tabtag-3": {
      "description": "Jump to tabtag 3"
    },
    "save-tabtag-4": {
      "description": "Save current tag to tabtag 4"
    },
    "open-tabtag-4": {
      "description": "Jump to tabtag 4"
    },
    "save-tabtag-5": {
      "description": "Save current tag to tabtag 5"
    },
    "open-tabtag-5": {
      "description": "Jump to tabtag 5"
    },
    "save-tabtag-6": {
      "description": "Save current tag to tabtag 6"
    },
    "open-tabtag-6": {
      "description": "Jump to tabtag 6"
    },
    "save-tabtag-7": {
      "description": "Save current tag to tabtag 7"
    },
    "open-tabtag-7": {
      "description": "Jump to tabtag 7"
    },
    "save-tabtag-8": {
      "description": "Save current tag to tabtag 8"
    },
    "open-tabtag-8": {
      "description": "Jump to tabtag 8"
    },
    "save-tabtag-9": {
      "description": "Save current tag to tabtag 9"
    },
    "open-tabtag-9": {
      "description": "Jump to tabtag 9"
    }
  }
}
